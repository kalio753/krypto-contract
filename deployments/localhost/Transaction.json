            block.timestamp,\\r\\n            keyword\\r\\n        );\\r\\n    }\\r\\n\\r\\n    function getAllTransactions()\\r\\n        public\\r\\n        view\\r\\n        returns (TransferStruct[] memory)\\r\\n    {\\r\\n        return transactions;\\r\\n    }\\r\\n\\r\\n    function getTransactionsCount() public view returns (uint256) {\\r\\n        return txCount;\\r\\n    }\\r\\n}\\r\\n\",\"keccak256\":\"0xec35c4c8baef64585b5c673340c75bba034d0b5b2484b24b07c05459686b55bc\",\"license\":\"MIT\"}},\"version\":1}",
